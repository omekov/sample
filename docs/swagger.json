                    "application/json"
                ],
                "tags": [
                    "sign"
                ],
                "summary": "Sign auth",
                "parameters": [
                    {
                        "description": "SignIn auth",
                        "name": "signin",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.SignInput"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.SignInput"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
