{
    "compilerOptions": {
        "outDir": "./dist",
        "target": "es5",
        "module": "esnext",
        "jsx": "react",
        "noImplicitAny": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "moduleResolution": "node",
        "baseUrl": "src",
        "paths": {
            "@components":["components"],
            "@pages":["pages"]
        }
    },
    "exclude": [
        "node_modules"
    ]
}